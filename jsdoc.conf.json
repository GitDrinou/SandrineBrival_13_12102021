{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }